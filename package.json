{
  "name": "react-test",
  "version": "1.0.0",
  "description": "WePaint - collaborative painting app",
  "engines": {
    "node": "8.9.1"
  },
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/tsc --module commonjs --outdir build/ --noImplicitAny --sourceMap --target ES5 --lib es6 server/index.ts && node build",
    "heroku-postbuild": "cd react-ui/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build && cd .."
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "express": "4.16.3",
    "ws": "6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mtreinik/react-test.git"
  },
  "keywords": [
    "Node",
    "React",
    "WebSocket",
    "TypeScript",
    "Heroku"
  ],
  "author": "Mikko Reinikainen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mtreinik/react-test/issues"
  },
  "homepage": "https://github.com/mtreinik/react-test#readme",
  "devDependencies": {
    "@types/express": "4.16.0",
    "@types/ws": "6.0.1",
    "typescript": "3.0.3"
  }
}
